<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cdoit.characterstrengthbuilder" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GoalsActivity"
            android:label="@string/app_name"
            android:parentActivityName=".MainActivity">
            <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".MainActivity" />

        </activity>
        <activity
            android:name=".HistoryActivity"
            android:label="@string/app_name"
            android:parentActivityName=".GoalsActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".GoalsActivity" />
        </activity>
        <activity
            android:name=".AnalysisActivity"
            android:label="@string/app_name"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".ProgressActivity"
            android:label="@string/app_name"
            android:parentActivityName=".AnalysisActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".AnalysisActivity" />
        </activity>
        <activity
            android:name=".WishActivity"
            android:label="@string/app_name"
            android:parentActivityName=".GoalsActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".GoalsActivity" />
        </activity>
    </application>

</manifest>
