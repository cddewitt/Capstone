<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff4e6"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.cdoit.characterstrengthbuilder.HistoryWoopDetailActivity">

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true"
            android:background="#fff4e6">

            <TextView
                android:id="@+id/lblDetailHistoryGoal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:text="Goal: "
                android:textSize="24dp" />

            <TextView
                android:id="@+id/tbxDetailHistoryGoal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_toRightOf="@+id/lblDetailHistoryGoal"
                android:text="New Text"
                android:textSize="24dp" />

            <TextView
                android:id="@+id/lblDetailHistoryResult"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/lblDetailHistoryGoal"
                android:layout_marginTop="50dp"
                android:text="Result: "
                android:textSize="24dp" />

            <TextView
                android:id="@+id/tbxDetailHistoryResult"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/lblDetailHistoryResult"
                android:layout_toRightOf="@+id/lblDetailHistoryResult"
                android:text="New Text"
                android:textSize="24dp" />

            <TextView
                android:id="@+id/lblDetailHistoryInteferencesAndPlans"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/lblDetailHistoryResult"
                android:layout_marginTop="50dp"
                android:text="Inteference:Plan "
                android:textSize="24dp" />

            <TextView
                android:id="@+id/tbxDetailHistoryInteferencesAndPlans"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lblDetailHistoryInteferencesAndPlans"
                android:layout_alignParentLeft="true"
                android:text="New Text"
                android:textSize="24dp" />

            <TextView
                android:id="@+id/lblDetailHistoryDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/tbxDetailHistoryInteferencesAndPlans"
                android:layout_marginTop="20dp"
                android:text="Deadline Date: "
                android:textSize="24dp" />

            <TextView
                android:id="@+id/tbxDetailHistoryDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/lblDetailHistoryDate"
                android:layout_toRightOf="@+id/lblDetailHistoryDate"
                android:text="New Text"
                android:textSize="24dp" />


            <TextView
                android:id="@+id/lblDetailHistoryTimeCreated"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/lblDetailHistoryDate"
                android:layout_marginTop="50dp"
                android:text="Created On: "
                android:textSize="24dp" />

            <TextView
                android:id="@+id/tbxDetailHistoryTimeCreated"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/lblDetailHistoryTimeCreated"
                android:layout_toRightOf="@+id/lblDetailHistoryTimeCreated"
                android:text="New Text"
                android:textSize="24dp" />

            <TextView
                android:id="@+id/lblDetailHistoryFinished"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/lblDetailHistoryTimeCreated"
                android:layout_marginTop="50dp"
                android:text="Completed On: "
                android:textSize="24dp" />

            <TextView
                android:id="@+id/tbxDetailHistoryFinished"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/lblDetailHistoryFinished"
                android:layout_toRightOf="@+id/lblDetailHistoryFinished"
                android:text="New Text"
                android:textSize="24dp" />

        </RelativeLayout>
    </ScrollView>
</RelativeLayout>

